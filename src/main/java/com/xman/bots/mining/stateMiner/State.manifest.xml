<manifest>
    <main-class>com.xman.bots.mining.stateMiner</main-class>
    <name>State Miner</name>
    <tag-line>Mine anywhere</tag-line>
    <!--Max of 50 chars-->
    <description>Mine Anywhere</description>
    <!--Max of 110 chars-->
    <version>0.2.0</version>
    <compatibility>
        <game>OSRS</game>
    </compatibility>
    <categories>
        <category>MINING</category>
    </categories>
    <!--Required to publish on the bot store-->
